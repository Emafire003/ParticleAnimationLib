{
	"schemaVersion": 1,
	"id": "animatedparticleslib",
	"version": "${version}",
	"name": "Animated Particles Lib",
	"description": "A particle animation library to create cool effects!",
	"authors": [
		"Emafire003, elBukkit"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "MIT",
	"icon": "assets/animatedparticleslib/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
          "me.emafire003.dev.animatedparticleslib.AnimatedParticlesLib"
		],
		"client": [
			"me.emafire003.dev.animatedparticleslib.ExampleModClient"
		]
	},
	"mixins": [
		"animatedparticleslib.mixins.json",
		{
			"config": "animatedparticleslib.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.0",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}